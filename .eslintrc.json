{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb-base"
    ],
    "env": {
        "browser": true
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": 0,
        "no-param-reassign": "off",
        "no-alert": "off",
        "no-undef": "off"
    }
}